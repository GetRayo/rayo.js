    "eslint-plugin-prettier": "^2.6.2",
    "express": "4.x",
    "fastify": "^1.6.0",
    "lerna": "2.x",
    "minimist": "1.x",
    "mocha": "5.x",
    "nyc": "12.x",
    "ora": "^3.0.0",
