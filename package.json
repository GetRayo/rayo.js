  },
  "homepage": "https://github.com/GetRayo/rayo.js",
  "scripts": {
    "eslint:fix": "eslint --quiet --fix .",
    "bench": "node test/benchmarks",
    "pretest": "lerna bootstrap",
    "test": "eslint --quiet . && nyc --reporter=lcov _mocha -R spec --timeout 30000 test/index.js",
    "posttest": "nyc report --reporter=text",
    "report": "codecov && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage ./.nyc_output",
    "copies": "cp readme.md ./packages/rayo/. && echo rayo/ compress/ send/ storm/ | xargs -n 1 -I file cp LICENSE ./packages/file",
    "prerelease": "npm test && npm run copies",
    "release": "lerna publish",
    "update": "npm-check -u"
  },
  "devDependencies": {
    "autocannon": "^2.4.1",
    "cli-table": "^0.3.1",
    "codecov": "^3.0.4",
    "coveralls": "^3.0.2",
    "dotenv": "^6.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "express": "^4.16.3",
    "fastify": "^1.9.0",
    "kleur": "^2.0.1",
    "lerna": "^3.0.6",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "npm-check": "^5.8.0",
    "nyc": "^12.0.2",
    "ora": "^3.0.0",
    "pancho": "^1.0.1",
    "polka": "^0.4.0",
    "prettier": "^1.14.2",
    "should": "^13.2.3",
    "sinon": "^6.1.5",
    "supertest": "^3.1.0"
  }
}
